                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Precision"
          }
        },
        "type": "object",
        "required": [
          "name",
          "display_name",
          "category",
          "precision"
        ],
        "title": "Model"
      },
      "ModelCategory": {
        "type": "string",
        "enum": [
          "classification",
          "detection"
        ],
        "title": "ModelCategory"
      },
